{
  "name": "nw_svelte_ts_vite",
  "version": "1.0.0",
  "description": "使用nw+svelte+typescript+vite实现跨平台桌面端应用",
  "main": "http://localhost:3000",
  "node-remote": "*://localhost/*",
  "build": {
    "nwFlavor": "sdk",
    "nwVersion": "0.14.7",
    "excludes": [
      "node_modules/**",
      "public/**",
      "src/**",
      ".gitignore",
      "index.html",
      "pnpm-lock.yaml",
      "README.md",
      "tsconfig.json",
      "vite.nw.build.ts"
    ],
    "strippedProperties": [
      "scripts",
      "devDependencies",
      "build",
      "node-remote",
      "dependencies",
      "keywords",
      "author",
      "license",
      "description"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx ts-node webpack.nw.build.ts --TARGET=dev",
    "build": "npx ts-node webpack.nw.build.ts",
    "win-x86": "build --tasks win-x86 ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^17.0.43",
    "@types/nw.gui": "^0.0.21",
    "css-loader": "^6.7.1",
    "esbuild": "^0.14.46",
    "esbuild-loader": "^2.19.0",
    "html-webpack-plugin": "^5.5.0",
    "nw": "0.65.1-sdk",
    "nwjs-builder-phoenix": "^1.15.0",
    "style-loader": "^3.3.1",
    "svelte": "^3.48.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.2"
  }
}
